{
  "name": "canvas-task-extension",
  "version": "1.3.8",
  "description": "A chrome extension that improves the Canvas LMS To Do sidebar!",
  "homepage": "https://github.com/jtcheng26/canvas-task-extension",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtcheng26/canvas-task-extension.git"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "node utils/build.js chrome",
    "firefox": "node utils/build.js firefox",
    "zip": "cd build; zip -r ../build.zip * -x \"build/.*\" -x \"build/__MACOSX\"",
    "start": "node utils/webserver.js",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,html}'",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "test": "jest",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.21.1",
    "d3-ease": "^3.0.1",
    "immutability-helper": "^3.1.1",
    "json-bigint": "^1.0.0",
    "react": "^17.0.1",
    "react-day-picker": "^8.1.3",
    "react-dom": "^17.0.1",
    "react-dom-confetti": "^0.2.0",
    "react-error-boundary": "^4.0.12",
    "react-move": "^6.5.0",
    "string-similarity": "^4.0.4",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@hot-loader/react-dom": "^17.0.1",
    "@storybook/addon-actions": "^7.6.6",
    "@storybook/addon-console": "^2.0.0",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/react-webpack5": "^7.6.6",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/chrome": "^0.0.154",
    "@types/d3-ease": "^3.0.0",
    "@types/jest": "^29.5.11",
    "@types/json-bigint": "^1.0.1",
    "@types/sinon-chrome": "^2.2.15",
    "@types/string-similarity": "^4.0.2",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "babel-loader": "^8.2.5",
    "babel-preset-react-app": "^10.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.1.1",
    "fs-extra": "^9.0.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0-alpha.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.1.2",
    "sinon-chrome": "^3.0.1",
    "source-map-loader": "^1.1.2",
    "storybook": "^7.6.6",
    "storybook-dark-mode": "^3.0.3",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.2",
