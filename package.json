    "typescript": "^5.3.3",
    "webpack": "^5.2.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/utils/styleMock.js",
      "^d3-ease$": "<rootDir>/node_modules/d3-ease/dist/d3-ease.min.js"
    },
    "testEnvironment": "jsdom"
  }
}
